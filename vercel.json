{
  "rewrites": [
    {
      "source": "/oauth/facebook",
      "destination": "/oauth.html"
    },
    {
      "source": "/oauth/google",
      "destination": "/oauth.html"
    },
    {
      "source": "/oauth",
      "destination": "/oauth.html"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}

